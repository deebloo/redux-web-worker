{
  "name": "redux-worker",
  "version": "1.0.0",
  "description": "A redux implementation with web workers",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run global-dev && npm run global-min && npm run commonjs",
    "global-min": "NODE_ENV=prod webpack",
    "global-dev": "NODE_ENV=dev webpack",
    "commonjs": "tsc --outDir build/",
    "release": "npm run build && npm publish",
    "test": "karma start",
    "postinstall": "typings install"
  },
  "keywords": [
    "web workers",
    "redux",
    "state managment"
  ],
  "files": [
    "build/src",
    "bundles/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deebloo/redux-workers.git"
  },
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/redux-workers/issues"
  },
  "homepage": "https://github.com/deebloo/redux-workers",
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "karma-jasmine": "^1.0.2"
  }
}
